public class Product {
	private int proId;
	private String prodName;
	private double prodPrice;
	//using setters and getters 
	public int getProId() {
		return proId;
	}
	public void setProId(int proId) {
		this.proId = proId;
	}
	public String getProdName() {
		return prodName;
	}
	public void setProdName(String prodName) {
		this.prodName = prodName;
	}
	public double getProdPrice() {
		return prodPrice;
	}
	public void setProdPrice(double prodPrice) {
		this.prodPrice = prodPrice;
	}
	
	

}
public class ProductMain {

	public static void main(String[] args) {
		Product pobj = new Product();						//creating object
		pobj.setProId(11233);
		pobj.setProdName("laptop");
		pobj.setProdPrice(23450);
		
		System.out.println("Enter Product Id "+pobj.getProId());
		System.out.println("Enter Product name "+pobj.getProdName());
		System.out.println("Enter Price "+pobj.getProdPrice());

	}

}
